{{title "Prijava"}}

<div class="row">
  <div class="col-md-4 col-md-offset-4">

    <div class="login">
      <img src="images/etf.png" alt="ETF" class="logo">

      {{#each flashMessages.queue as |flash|}}
        {{flash-message flash=flash}}
      {{/each}}

      <div class="panel panel-primary panel-login">
        <div class="panel-body">
          {{login-form authHandler=(action "loginNormal") identification=identification password=password disabled=disabled}}
        </div>
      </div>
    </div>
  </div>
</div>

